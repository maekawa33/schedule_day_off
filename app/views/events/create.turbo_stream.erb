<%= turbo_stream.update dom_id(Event.new,dom_id(@schedule)), "" %>

<%= turbo_stream.update dom_id(@schedule, :swiper) do%>
    <%= render 'schedules/swiper', size: "show", schedule: @schedule%>
<% end %>

<%= turbo_stream.update dom_id(@schedule) do%>
    <%= render 'schedules/schedule_events', schedule: @schedule, events: @events%>
<% end %>